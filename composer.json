{
  "name": "msbios/doctrine",
  "description": "MSBios Doctrine",
  "license": "MIT",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-doctrine",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Doctrine\\": "tests/"
    }
  },
  "require": {
    "ramsey/uuid-doctrine": "^1.5",
    "doctrine/doctrine-module": "^2.1",
    "msbios/json": "^2.0",
    "msbios/stdlib": "^2.0",
    "msbios/form": "^2.0",
    "oro/doctrine-extensions": "^1.3"
  },
  "require-dev": {
    "msbios/test": "^2.0",
    "msbios/debug": "^2.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\Doctrine",
      "config-provider": "MSBios\\Doctrine\\ConfigProvider"
    }
  }
}
